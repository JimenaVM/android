<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:weightSum="1">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_weight="0.55">

        <FrameLayout
            android:layout_width="180dp"
            android:layout_height="127dp"
            android:layout_gravity="center_horizontal|bottom">

            <ImageView
                android:layout_width="117dp"
                android:layout_height="66dp"
                android:id="@+id/imgNoEje"
                android:layout_gravity="center_horizontal|bottom"
                android:src="@drawable/noej"
                android:onClick="@layout/sin_ejecutar"/>

            <ImageView
                android:layout_width="86dp"
                android:layout_height="65dp"
                android:id="@+id/imageView5"
                android:layout_gravity="center_horizontal|top"
                android:src="@drawable/noe"
                android:layout_column="30" />

        </FrameLayout>

        <FrameLayout
            android:layout_width="179dp"
            android:layout_height="140dp"
            android:layout_gravity="center_horizontal|top"
            >

            <ImageView
                android:id="@+id/imageView6"
                android:layout_gravity="left|top"
                android:src="@drawable/ejec"
                android:layout_column="1"
                android:layout_span="13"
                android:layout_marginLeft="50dp"
                android:layout_width="60dp"
                android:layout_height="60dp" />

            <ImageView
                android:layout_width="120dp"
                android:layout_height="77dp"
                android:id="@+id/imgEje"
                android:layout_gravity="center_horizontal|bottom"
                android:src="@drawable/eeee" />

        </FrameLayout>

    </FrameLayout>



</LinearLayout>